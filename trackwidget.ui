QComboBox
{
    background-color: #FFFFFF;
    color:rgb(30, 30, 30);
    border: 1px solid rgb(225,230,234);
	border-radius:5px;
    padding-left: 10px;
    min-width: 150px;
    min-height: 25px;
}

/* 下拉箭头图标样式 */
QComboBox::down-arrow 
{
    image: url(:/image/combobox-arrow.png);  /* 灰色箭头图标 */
}

/* 下拉箭头悬停效果 */
QComboBox::drop-down:hover
 {
    background-color: rgb(245, 245, 245);  /* 浅灰悬停效果 */
}

/* 下拉列表样式 */
QComboBox QAbstractItemView 
{
    background-color: #FFFFFF;  /* 白色背景 */
    color: rgb(30, 30, 30);  /* 与原文字同色 */
    border: 1px solid rgb(225, 230, 234);  /* 与原边框一致 */
    border-radius: 5px;  /* 圆角匹配 */
    outline: none;  /* 去除选中虚线框 */
    margin-top: 2px;  /* 与主控件间距 */
    /* 下拉列表的顶部圆角需要特殊处理 */
    border-top-left-radius: 0;
    border-top-right-radius: 0;
}

/* 下拉列表中的项 */
QComboBox QAbstractItemView::item 
{
    height: 25px;  /* 项高度 */
    padding:5px;
}

/* 下拉列表中的选中项 */
QComboBox QAbstractItemView::item:selected 
{
    background-color: rgb(240, 245, 250);  /* 浅蓝选中背景 */
    color: rgb(70, 70, 70);  /* 加深文字颜色 */
}

/* 禁用状态样式 */
QComboBox:disabled
{
    background-color: rgb(245, 245, 245);
    color: rgb(180, 180, 180);
    border: 1px solid rgb(235, 235, 235);
}


QLineEdit 
{
    background-color: #FFFFFF;
    color: rgb(30, 30, 30);
    border: 1px solid rgb(225, 230, 234);
    border-radius: 5px;
    padding: 5px 8px;  /* 比QComboBox稍宽的内边距 */
    min-height: 15px;
    selection-background-color: rgb(70, 130, 200);  /* 选中文本背景色 */
    selection-color: white;  /* 选中文本颜色 */
}

/* 聚焦状态 */
QLineEdit:focus {
    background-color: #FFFFFF;  /* 保持白色背景 */
}</string>
